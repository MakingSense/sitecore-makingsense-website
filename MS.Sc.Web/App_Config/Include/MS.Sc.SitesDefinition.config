<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sites>
      <site name="makingsensesc" patch:before="site[@name='website']"
            hostName="makingsensesc|makingsensesc.southcentralus.cloudapp.azure.com|localhost"
            virtualFolderenableTracking="true"
            virtualFolder="/"
            physicalFolder="/"
            rootPath="/sitecore/content/Websites/Making Sense"
            startItem="/home"
            database="web"
            domain="extranet"
            allowDebug="true"
            cacheHtml="true"
            htmlCacheSize="50MB"
            registryCacheSize="0"
            viewStateCacheSize="0"
            xslCacheSize="25MB"
            filteredItemsCacheSize="10MB"
            enablePreview="true"
            enableWebEdit="true"
            enableDebugger="true"
            disableClientData="false"
            cacheRenderingParameters="true"
            renderingParametersCacheSize="10MB"/>
    </sites>
  </sitecore>
</configuration>